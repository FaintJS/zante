{
    "name": "zante",
    "version": "0.2.0",
    "description": "buil tool on the top of webpack",
    "main": "index.js",
    "bin": {
        "zante": "./dist/bin/zante.js"
    },
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "clean": "rimraf ./dist",
        "copy": "cpx \"src/**/!(*.ts?(x)|*.js?(x))\" dist",
        "copy:watch": "cpx \"src/**/!(*.ts?(x)|*.js?(x))\" dist --watch",
        "dev": "npm run clean && concurrently \"tsc -w\" \"npm run copy:watch\"",
        "build": "npm run lint && npm run clean && tsc && npm run copy"
    },
    "repository": "https://github.com/FaintJS/zante.git",
    "author": "carrytao",
    "license": "MIT",
    "dependencies": {
        "autoprefixer": "^7.1.0",
        "babel-core": "^6.24.1",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.0.0",
        "babel-preset-env": "^1.5.1",
        "babel-preset-stage-0": "^6.24.1",
        "chalk": "^1.1.3",
        "cheerio": "^0.22.0",
        "commander": "^2.9.0",
        "css-loader": "^0.28.1",
        "eslint": "^3.19.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-loader": "^1.7.1",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-node": "^4.2.2",
        "eslint-plugin-promise": "^3.5.0",
        "eslint-plugin-standard": "^3.0.1",
        "express": "^4.15.2",
        "file-loader": "^0.11.1",
        "happypack": "^3.0.3",
        "html-webpack-plugin": "^2.28.0",
        "http-proxy-middleware": "^0.17.4",
        "json-loader": "^0.5.4",
        "less": "^2.7.2",
        "less-loader": "^4.0.3",
        "lodash": "^4.17.4",
        "openurl": "^1.1.1",
        "postcss": "^6.0.1",
        "postcss-loader": "^2.0.3",
        "raw-loader": "^0.5.1",
        "react-hot-loader": "^3.0.0-beta.6",
        "shelljs": "^0.7.7",
        "style-loader": "^0.17.0",
        "url-loader": "^0.5.8",
        "webpack": "^2.5.1",
        "webpack-dev-middleware": "^1.10.2",
        "webpack-dev-server": "^2.4.5",
        "webpack-hot-middleware": "^2.18.0",
        "webpack-merge": "^4.1.0"
    },
