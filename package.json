        "@types/handlebars": "^4.0.32",
        "@types/lodash": "^4.14.64",
        "@types/react": "^15.0.24",
        "@types/react-dom": "^15.5.0",
        "@types/react-router": "3.0.5",
        "@types/shelljs": "^0.7.1",
        "@types/webpack": "^2.2.15",
        "@types/webpack-env": "^1.13.0",
        "babel-eslint": "^7.2.3",
        "concurrently": "^3.4.0",
        "cpx": "^1.5.0",
        "eslint": "^3.19.0",
        "eslint-config-standard": "^10.2.1",
        "eslint-plugin-import": "^2.2.0",
