{
    "compilerOptions": {
        "baseUrl": ".",
        "module": "commonjs",
        "target": "ES6",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "allowJs": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "lib": [
            "es2015",
            "dom"
        ],
        "jsx": "react"
    },
    "include": [
      "src/**/*"
    ]
}
